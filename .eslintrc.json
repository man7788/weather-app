{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "no-use-before-define": "off"
    // "prefer-const": "off",
    // "import/prefer-default-export": "off"
  }
}
